{
  "type": "ecs-serviceconnect",
  "name": "serviceconnect-cni-test-config",
  "cniVersion": "0.3.0",
  "ingressConfig": [
    {
      "interceptPort": 8080,
      "listenerName": "inbound_abc",
      "listenerPort": 30000
    },
    {
      "interceptPort": 8090,
      "listenerName": "inbound_bcd",
      "listenerPort": 30001
    }
  ],
  "egressConfig": {
    "listenerName": "outbound_listener",
    "listenerPort": 30002,
    "vip": {
      "ipv4Cidr": "127.255.0.0/16",
      "ipv6Cidr": "127.255.0.0"
    },
    "redirectMode": "nat"
  },
  "enableIPV4": true,
  "enableIPv6": true
}